datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

generator photonjs {
  provider = "photonjs"
}

model User {
  id                String   @default(cuid()) @id @unique
  createdAt         DateTime @default(now())
  username          String   @unique
  email             String   @unique
  password          String?
  authType          String   @default("classic")
  idToken           String?
}
